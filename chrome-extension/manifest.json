{
    "manifest_version": 2,
    "name": "DevProgress leaderboard",
    "version": "0.1",
    "description": "Logs calling activities to DevProgress",
    "icons": {
        "128": "dev_progress128.png"
    },
    "background": {
        "persistent": false,
        "scripts": [
            "bg.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.votebuilder.com/MyListVirtualPhoneBank.aspx",
                "https://www.votebuilder.com/ContactDetailScript.aspx",
                "https://www.votebuilder.com/AutoDialDisposition.aspx",
                "http://localhost:8000/*"
            ],
            "js": [
                "bower_components/keen-js/dist/keen-tracker.min.js",
                "bower_components/jquery-1.11.1/dist/jquery.min.js",
                "keen.js",
                "votebuilder.js"
            ],
            "all_frames": true
        }
    ],
    "permissions": [
        "http://localhost:8000/*",
        "https://www.votebuilder.com/MyListVirtualPhoneBank.aspx",
        "https://www.votebuilder.com/ContactDetailScript.aspx"
    ],
    "browser_action": {
        "default_title": "",
        "default_icon": "dev_progress19.png",
        "default_popup": "popup.html"
    }
}
